{
  "name": "@rhangai/validator",
  "version": "0.2.26",
  "description": "Validator",
  "keywords": [
    "node",
    "validator",
    "decorator",
    "sanitizer",
    "transformer"
  ],
  "author": "rhangai <hangai.renan@gmail.com>",
  "homepage": "https://github.com/rhangai/node-packages/tree/main/packages/node-validator#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "bugs": {
    "url": "https://github.com/rhangai/node-packages/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhangai/node-packages.git"
  },
  "devDependencies": {
    "@rhangai/common": "^0.2.15",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.3.2"
  },
  "peerDependencies": {
    "@rhangai/common": "^0.2.5",
    "reflect-metadata": "^0.1.13"
  },
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint src --ext js,jsx,ts,tsx",
    "lint:fix": "yarn run -s lint --fix",
    "preversion": "yarn run -s build",
    "test": "jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFiles": [
      "reflect-metadata",
      "@rhangai/common/setup"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx}"
    ]
  }
}
