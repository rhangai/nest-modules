{
  "name": "@rhangai/validator",
  "version": "0.1.1",
  "description": "Validator",
  "keywords": [
    "nest",
    "util",
    "utility"
  ],
  "author": "rhangai <hangai.renan@gmail.com>",
  "homepage": "https://github.com/rhangai/node-nest-modules#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "bugs": {
    "url": "https://github.com/rhangai/node-nest-modules/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhangai/node-nest-modules.git"
  },
  "devDependencies": {
    "@rhangai/common": "^0.1.1",
    "@types/jest": "^26.0.23",
    "jest": "^27.0.4",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2"
  },
  "peerDependencies": {
    "@rhangai/common": "^0.1.1",
    "reflect-metadata": "^0.1.13"
  },
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint src",
    "lint:fix": "yarn run -s lint --fix",
    "preversion": "yarn run -s build",
    "test": "jest",
    "test:coverage": "jest --collect-coverage"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFiles": [
      "reflect-metadata",
      "@rhangai/common/setup"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx}"
    ]
  }
}
