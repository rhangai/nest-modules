{
  "name": "@rhangai/common",
  "version": "0.5.0",
  "description": "Some common functionality for my modules",
  "keywords": [],
  "author": "rhangai <hangai.renan@gmail.com>",
  "homepage": "https://github.com/rhangai/node-packages/tree/main/packages/common#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "bugs": {
    "url": "https://github.com/rhangai/node-packages/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhangai/node-packages.git"
  },
  "dependencies": {
    "bignumber.js": "^9.0.1",
    "dayjs": "^1.10.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "jest": "^27.0.4",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2"
  },
  "peerDependencies": {
    "tslib": "^2"
  },
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint src --ext js,jsx,ts,tsx",
    "lint:fix": "yarn run lint --fix",
    "preversion": "yarn run build",
    "test": "jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/setup.js"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/setup.js",
      "<rootDir>/lib",
      "/node_modules/"
    ]
  }
}
