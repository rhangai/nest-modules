{
  "name": "@rhangai/common",
  "version": "0.9.2",
  "description": "Some common functionality for my modules",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "rhangai <hangai.renan@gmail.com>",
  "homepage": "https://github.com/rhangai/node-packages/tree/main/packages/common#readme",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "bugs": {
    "url": "https://github.com/rhangai/node-packages/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhangai/node-packages.git"
  },
  "dependencies": {
    "bignumber.js": "^9.1.2",
    "dayjs": "^1.11.10"
  },
  "devDependencies": {
    "tsup": "*",
    "typescript": "*"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "yarn run build:ts && yarn run build:types",
    "build:ts": "tsup-node -d lib --sourcemap true --format esm,cjs src/index.ts",
    "build:types": "tsup-node -d lib src/index.ts --dts-only"
  }
}
