{
  "name": "@rhangai/nest-util",
  "version": "0.9.4",
  "description": "Nest utilities",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "rhangai <hangai.renan@gmail.com>",
  "homepage": "https://github.com/rhangai/node-packages/tree/main/packages/nest-util#readme",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./lib/index.mjs",
      "default": "./lib/index.js"
    },
    "./auth": {
      "types": "./src/auth/index.ts",
      "import": "./lib/auth.mjs",
      "default": "./lib/auth.js"
    },
    "./graphql": {
      "types": "./src/graphql/index.ts",
      "import": "./lib/graphql.mjs",
      "default": "./lib/graphql.js"
    },
    "./typeorm": {
      "types": "./src/typeorm/index.ts",
      "import": "./lib/typeorm.mjs",
      "default": "./lib/typeorm.js"
    }
  },
  "bugs": {
    "url": "https://github.com/rhangai/node-packages/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhangai/node-packages.git"
  },
  "devDependencies": {
    "@nestjs/graphql": "*",
    "@rhangai/common": "^0.9.2",
    "tsup": "*",
    "typeorm": "*",
    "typescript": "*"
  },
  "peerDependencies": {
    "@nestjs/common": "^10",
    "@nestjs/graphql": ">=10",
    "@rhangai/common": "^0.9.2",
    "graphql": ">=15",
    "typeorm": ">=0.2"
  },
  "peerDependenciesMeta": {
    "@nestjs/graphql": {
      "optional": true
    },
    "graphql": {
      "optional": true
    },
    "typeorm": {
      "optional": true
    }
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "tsup-node -d lib --sourcemap true --format esm,cjs",
    "typecheck": "tsc --noEmit"
  }
}
